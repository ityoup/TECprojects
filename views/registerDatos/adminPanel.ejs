                        <script>
                            // Obtener referencia al input de archivo
                            const fileInput = document.getElementById('formFileLg');
    
                            // Agregar un evento de cambio al input de archivo
                            fileInput.addEventListener('change', function (event) {
                                const file = event.target.files[0]; // Obtener el archivo seleccionado
    
                                if (file) {
                                    const reader = new FileReader();
    
                                    // Definir la función a ejecutar cuando se haya leído el archivo
                                    reader.onload = function (e) {
                                        const profilePicture = document.getElementById('profilePicture');
                                        profilePicture.src = e.target.result; // Actualizar la imagen con el contenido del archivo
                                    };
    
                                    reader.readAsDataURL(file); // Leer el archivo como URL de datos
                                }
                            });
                        </script>
    
