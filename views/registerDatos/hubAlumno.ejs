}

.toggle--switch {
position: absolute;
width: 18px;
height: 18px;
background-color: white;
border-radius: 18px;
top: 2px;
left: 2px;
-webkit-transition: all 0.3s cubic-bezier(0.42, 0, 0, 1);
transition: all 0.3s cubic-bezier(0.42, 0, 0, 1);
}

.toggle.active {
background-color: #3b82f6;
}

.toggle.active .toggle--switch {
left: calc(100% - 20px);
}

